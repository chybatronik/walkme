<h1>Listing tasks</h1>

<table class="table table-bordered table-striped">
  <tr>
    <th>Name</th>
    <th>Text</th>
    <th>catalog_id</th>
    <th>Url</th>
    <th>Object</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tasks.each do |task| %>
  <tr>
    <td><%= task.name %></td>
    <td><%= task.text %></td>
    <td><%= task.catalog_id %></td>
    <td><%= task.url %></td>
    <td><%= task.object %></td>
    <td><%= link_to 'Show', task %></td>
    <td><%= link_to 'Edit', edit_task_path(task) %></td>
    <td><%= link_to 'Destroy', task, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Task', new_task_path %>
