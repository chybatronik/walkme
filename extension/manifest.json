{
  "manifest_version": 2,

  "name": "Walkme",
  "description": "This extension for walme.",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://127.0.0.1:3000/", "debugger", "background", "tabs"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self' ",

  "content_scripts": [ {
    "matches": ["http://*/*"],
    "css":["fix.css"],
    "js": ["lib/jquery-1.9.1.min.js", "page.js"]
  }
  ]
}
